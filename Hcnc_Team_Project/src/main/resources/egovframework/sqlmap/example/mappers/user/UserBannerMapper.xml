<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user.mapper.UserBannerMapper">
	<select id="selectBannerListByUser" resultType="java.util.Map">
		SELECT *
		FROM BANNERS
		WHERE IS_VISIBLE = 1
		<if test="bannerType != null and bannerType != ''">
	        AND BANNER_TYPE = #{BANNER_TYPE}
	    </if>
	</select>
</mapper>