        <Calendar id="Calendar00_01" taborder="2" left="907" top="16" width="120" height="30" onchanged="search_area_Calendar00_01_onchanged"/>
        <Static id="txt_th00_00_00" taborder="3" text="-" left="1052" top="12" width="80" height="36" font="normal 11pt/normal &quot;Noto Sans KR&quot;" onclick="search_area_txt_th_onclick"/>
        <Calendar id="Calendar00_00_00" taborder="4" left="1075" top="16" width="120" height="30" onchanged="search_area_Calendar00_00_00_onchanged"/>
